{
    "openapi": "3.0.0",
    "info": {
        "title": "Example API",
        "description": "API for the Example application",
        "version": "1.0.0"
    },
    "paths": {
        "/login": {
            "get": {
                "tags": [
                    "Auth"
                ],
                "summary": "Show login form",
                "description": "Displays the login form for the user",
                "operationId": "5db83a1e036617251edf69345e97b0e0",
                "responses": {
                    "200": {
                        "description": "Login form displayed"
                    }
                }
            },
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Login",
                "description": "Logs in a user",
                "operationId": "ce5ef90f2ad7d4ab966397fd8ddd231d",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "email"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "302": {
                        "description": "Redirect response",
                        "headers": {
                            "Location": {
                                "description": "Redirect URL",
                                "schema": {
                                    "type": "string"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        },
                                        "errors": {
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/logout": {
            "get": {
                "tags": [
                    "Auth"
                ],
                "summary": "Logout",
                "description": "Logs out the currently authenticated user",
                "operationId": "86bc06af2fef4022888cc36cef6bf3d8",
                "responses": {
                    "200": {
                        "description": "Logout successful"
                    }
                }
            }
        }
    }
}